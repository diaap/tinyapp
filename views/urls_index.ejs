<!DOCTYPE html>
  <head>
  <title>Tiny App</title>
  </head>
  <body>
  <header>
    <% include ./partials/_headers %>
  </header>
    <p>
      <ul>
        <% for(let key in url) { %>
      <li><%= key %> <%= url[key].url %></li>

        <% if (user && url[key].linkid === username.id) { %>
          <form action="/urls/<%= key  %>" method="POST">
            <input id="newURL" type="text" name="newURL" value="<%= url[key].longURL %>">
          </form>

            <form action="/urls/<%= key %>/delete" method="POST">
              <input type="submit" name="newURL" value='Delete'>
              <a href="/urls/<%= key %>"> Edit </a>
            </form>

          <% } %>
         <% } %>
      </ul>
      </p>
      <a href = "/urls/new"> Want to shorten a new URL? </a>
    </body>
